{
    "env": {
      "es2021": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "requireConfigFile": false,
      "parser": "@babel/eslint-parser"
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "vue/multi-word-component-names": "off"
    }
  }